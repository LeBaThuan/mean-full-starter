{
  "name": "mean-full-starter",
  "version": "1.0.0",
  "author": "Stanley Eosakul",
  "description": "MEAN Stack template with features including JWT authentication, MongoDB Atlas, Amazon S3 storage, Stripe API, and Angular Universal",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "serve": "ng serve --aot",
    "start": "node ./node_src/server.js",
    "build": "ng build --prod && ng build --prod --app universal --output-hashing=none",
    "build-prod": "ng build --prod && ng build --prod --app universal --output-hashing=none && node server.js",
    "heroku-postbuild": "ng build --prod && ng build --prod --app universal --output-hashing=none",
    "start-ts": "tsc -p ./node_src_ts/tsconfig.server.json && node ./node_build_ts/server.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "5.2.9",
